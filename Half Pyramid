//Pattern 1:
//Hollow Rectangular Pattern

#include<stdio.h>

int main(){
    for(int i=5;i>0;i--){
        for(int j=0;j<i;j++){
            printf("* ");
        }
        printf("\n");
    }
}


////////
#include<stdio.h>

int main(){
    for(int i=0;i<=5;i++){
       for(int j=0;j<=5;j++){
           if(j<(5-i)){
               printf(" ");
           }else{
               printf("*");
           }
       }
       printf("\n");
   }
}
